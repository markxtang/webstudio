{
  "name": "@webstudio-is/ai",
  "version": "0.1.0",
  "description": "Webstudio AI Tools",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "checks": "pnpm typecheck && pnpm lint",
    "dev": "build-package --watch",
    "build": "pnpm build:prompts && pnpm build:prompt-theme-types && build-package",
    "build:prompts": "tsx ./bin/prompts.ts \"./src/chains/**/*.prompt.md\"",
    "build:prompt-theme-types": "tsx ./bin/prompt-theme-types.ts",
    "dts": "tsc --declarationDir lib/types",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "devDependencies": {
    "@webstudio-is/jest-config": "workspace:^",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "jest": "^29.3.1",
    "tsx": "^3.12.6",
    "type-fest": "^3.7.1",
    "typescript": "5.1.3"
  },
  "exports": {
    "source": "./src/index.ts",
    "import": "./lib/index.js",
    "require": "./lib/cjs/index.js"
  },
  "types": "lib/types/index.d.ts",
  "files": [
    "lib/*",
    "src/*",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "dependencies": {
    "@babel/parser": "^7.22.7",
    "@babel/types": "^7.22.5",
    "@webstudio-is/css-data": "workspace:^",
    "@webstudio-is/project": "workspace:^",
    "@webstudio-is/project-build": "workspace:^",
    "@webstudio-is/react-sdk": "workspace:^",
    "escape-string-regexp": "^5.0.0",
    "json5": "^2.2.3",
    "mdast-util-from-markdown": "^1.3.0",
    "openai": "^3.2.1",
    "unist-util-visit": "^4.1.2",
    "z": "^1.0.9",
    "zod-to-ts": "^1.1.4"
  }
}
